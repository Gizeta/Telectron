{
  "name": "telectron",
  "version": "0.0.1",
  "description": "Simple VoIP client on Electron",
  "main": "bootstrapper.js",
  "scripts": {
    "pack": "./node_modules/.bin/electron-packager . Telectron --platform=win32 --arch=x64 --ignore=pjsip --ignore=pjsua-wrapper --overwrite",
    "rebuild": "./node_modules/.bin/electron-rebuild -w ffi -p",
    "start": "./node_modules/.bin/electron .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gizeta/Telectron.git"
  },
  "keywords": [
    "VoIP",
    "Electron"
  ],
  "author": "Gizeta",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Gizeta/Telectron/issues"
  },
  "homepage": "https://github.com/Gizeta/Telectron#readme",
  "devDependencies": {
    "electron": "^1.4.6",
    "electron-packager": "^8.2.0",
    "electron-rebuild": "^1.3.0"
  },
  "dependencies": {
    "babel-preset-node6": "^11.0.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "ffi": "^2.2.0",
    "material-ui": "^0.15.4",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-tap-event-plugin": "^1.0.0"
  }
}
